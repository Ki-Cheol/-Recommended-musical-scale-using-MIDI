                    <MenuItem Header="오선 삭제"/>
                </ContextMenu>
            </Canvas.ContextMenu>
            <Line X1="70" Y1="58" X2="2000" Y2="58" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="58" Name="line1"></Line>
            <Line X1="70" Y1="66" X2="2000" Y2="66" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="66" Name="line2"></Line>
            <Line X1="70" Y1="74" X2="2000" Y2="74" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="74" Name="line3"></Line>
            <Line X1="70" Y1="82" X2="2000" Y2="82" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="82" Name="line4"></Line>
            <Line X1="70" Y1="90" X2="2000" Y2="90" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="90" Name="line5"></Line>


            <Line X1="70" Y1="58" X2="70" Y2="90" Stretch="Fill" Stroke="Black" Canvas.Left="70" Canvas.Top="58" Name="line11"></Line>

        </Canvas>
    </Grid>
</UserControl>
