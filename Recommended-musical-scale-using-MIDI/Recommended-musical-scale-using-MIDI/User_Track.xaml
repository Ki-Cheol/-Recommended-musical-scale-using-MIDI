        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1037*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Canvas Name="canvas1" AllowDrop="True" Background="White" Height="140" Width="1040" Grid.ColumnSpan="2">
